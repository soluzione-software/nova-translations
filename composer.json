{
    "name": "soluzione-software/nova-translations",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "^6.0|^7.0|^8.0",
        "laravel/nova": "~2.0|~3.0",
        "soluzione-software/laravel-translations": "^0.2"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SoluzioneSoftware\\NovaTranslations\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SoluzioneSoftware\\NovaTranslations\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
